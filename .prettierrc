{
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "endOfLine": "auto",
  "importOrder": [
    "^react$",
    "^@react$",
    "<THIRD_PARTY_MODULES>",
    "^[./]",
    "^@/(.*)$",
    "^@assets/(.*)$",
    "^@components/(.*)$",
    "^@configs/(.*)$",
    "^@features/(.*)$",
    "^@hooks/(.*)$",
    "^@lib/(.*)$",
    "^@seo/(.*)$",
    "^@services/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
